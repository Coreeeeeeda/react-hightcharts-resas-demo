{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es6",
      "dom",
    ],
    "jsx": "react",
    "module": "commonjs",
    "rootDir": "src",
    "outDir": "public",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "exclude": ["node_modules", "public", "webpack"],
  "typeRoots": [
    "types"
  ],
}
